@model  IEnumerable<PL.Models.ResultViewModel>
<h2></h2>
@if (Model.Count() > 0)
{
    <table class="table">

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.Raw(ViewBag.Topic)

                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.Date)

                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.Score)/ @Html.Raw(ViewBag.Count)
                </td>
            </tr>
        }


    </table>
    @Html.ActionLink("Вернуться к списку тестов", "Index")
}
else
{
    <h1>Статистика по данному тесту отстутствует</h1>
    @Html.ActionLink("Вернуться к списку тестов", "Index")
}